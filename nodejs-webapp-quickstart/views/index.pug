extends layout

block content
  .main-container
    .cloud-image
        img(src='/images/successCloudNew.svg')
    .content
        .content-body
            .success-text
                p Welcome!
            .description
                p The QuickStart WebApp has successfully setup

  .row
    .col-md-4
      h2 Link to other web pages
      p This will leave your webapp, but provides options to users.
      p
        a.btn.btn-default(href='https://google.com/') Google Search &raquo;
    .col-md-4
      h2 Link to other pages
      p Redirect to another view/web page defined in your webapp
      p
        a.btn.btn-default(href='/login') Login Page &raquo;
